{
  "packages": ["apps/*", "libs/*"],
  "version": "independent",
  "command": {
    "publish": {
      "conventionalCommits": true,
      "yes": true
    },
    "version": {
      "message": "chore(release): publish"
    }
  },
  "changelog": {
    "labels": {
      "feature": "New Feature",
      "core": "Core",
      "bug": "Bug Fix"
    }
  }
}
